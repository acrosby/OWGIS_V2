				defParticleSpeed=".03"/>
		<layer Menu="prev3,atm,psfc" EN="Surface Pressure"
			   ES="Presion" name="atm_forecast_prev3/PSFC" />
		<layer Menu="prev3,atm,sst" EN="Sea Surface Temperature" ES="Temperatura superfical del mar"
			   name="atm_forecast_prev3/SST" />
