<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
	<!-- Atmosphere layers -->
    <MainLayers BBOX="-123.828622,-2.459646,-58.255699,48.7692"
				format="image/gif"  server="http://ncwms.coaps.fsu.edu/ncWMS/wms"
				ncWMS="true" >  
		<layer Menu="prev3,atm,t2" EN="Air Temperature at 2m"
			   ES="Temperatura a 2mts" name="atm_forecast_prev3/T2" />
